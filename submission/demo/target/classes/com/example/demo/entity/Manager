package com.example.demo.entity;

import javax.persistence.Entity;
import javax.persistence.Id;
import java.util.UUID;

@Entity
public class Manager {
    @Id
    @GeneratedValue
    private UUID managerId;
    
    private String name;
    
    private boolean isActive = true;

    // Getters and Setters
}
